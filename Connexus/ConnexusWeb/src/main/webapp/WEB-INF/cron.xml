<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/updateleaderboard</url>
        <description>Update the leaderboard statistics and email reports.</description>
        <schedule>every 5 minutes</schedule>
    </cron>
    <cron>
        <url>/cron/updateautocomplete</url>
        <description>Rebuild the autocompletion index at the top of ever hour.</description>
        <schedule>every 1 hours synchronized</schedule>
    </cron>
</cronentries>
